time - O(sqrt n), space - O(1)

#include <iostream>
using namespace std;

// Function to check if a number is prime
bool isPrime(int n) {
    if (n <= 1)
        return false;  // 0 and 1 are not prime

    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0)
            return false;  // divisible by i → not prime
    }
    return true;  // no divisors found → prime
}

